#install a tldr instance
curl -o ~/bin/tldr https://raw.githubusercontent.com/raylee/tldr/master/tldr
chmod +x ~/bin/tldr

#setup defaault ssh params
curl -o ~/.ssh/config https://gist.githubusercontent.com/sschlesier/128b3c59ed82034d2fc983ecabdfb4ea/raw/e3fb684e26914335c2d79b40764aceda3c468697/config
chmod 644 ~/.ssh/config
